          <Configuration>
            <CalloutSymbol BorderBrushColor="#0000FF" ForegroundColor="#FFFFFF" FillColor="#0000FF" FillOpacity="0.90" />
            <OutlineSymbol Color="#0000FF" Width="1" />
          </Configuration>
        </View>
      </Views>
