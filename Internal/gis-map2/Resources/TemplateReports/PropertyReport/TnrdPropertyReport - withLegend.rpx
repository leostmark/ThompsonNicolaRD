  <Script><![CDATA[private void setVisible(string table, string label) {
		DataTable tableDetails =  (DataTable)((DataSet)rpt.DataSource).Tables[table];
		Label reportLabel = (Label)rpt.Sections["Detail1"].Controls[label];
		if  (null == tableDetails || tableDetails.Rows.Count == 0) {
		    reportLabel.Visible = true;
		 }
}
		
private void setIntersect(string table, string label) {
			DataTable tableDetails =  (DataTable)((DataSet)rpt.DataSource).Tables[table];
			Label reportLabel = (Label)rpt.Sections["Detail1"].Controls[label];
			if  (null == tableDetails || tableDetails.Rows.Count == 0) {
				reportLabel.Value = "No";
			}  else {
				reportLabel.Value = "Yes";
			}
}

private void setDefault(string table) {
			DataTable tableDetails =  (DataTable)((DataSet)rpt.DataSource).Tables[table];
			if  (null != tableDetails && tableDetails.Rows.Count == 0) {
					((DataTable)((DataSet)rpt.DataSource).Tables[table]).Rows.Add();
			}
}

private void setJoin(string table, string column, string field) {
			DataTable tableDetails =  (DataTable)((DataSet)rpt.DataSource).Tables[table];
			TextBox reportField = (TextBox)rpt.Sections["Detail1"].Controls[field];
			if  (null != tableDetails && tableDetails.Rows.Count > 0) {
				Hashtable values = new Hashtable();
				foreach(DataRow row in tableDetails.Rows) {
					if (!values.Contains(row[column])) {
						values.Add(row[column], row[column]);
					}
				}
				String str = "";
				foreach(Object value in values.Values) {
					if (!str.Equals("")) {
						str += ",";
					}
					str += value.ToString();
				}
				reportField.Value = str;
			}  else {
				reportField.Value = "N/A";
			}
}

private void setFirst(string table, string column, string field) {
			DataTable tableDetails =  (DataTable)((DataSet)rpt.DataSource).Tables[table];
			TextBox reportField = (TextBox)rpt.Sections["Detail1"].Controls[field];
			if  (null != tableDetails && tableDetails.Rows.Count > 0) {
				reportField.Value =  tableDetails.Rows[0][column];
			}  else {
				reportField.Value = "";
			}
	
}

public bool ActiveReport_FetchData(bool eof)
{	

		Picture featureMapImage = rpt.Sections["Detail1"].Controls["PropertyFeatureMap"] as Picture;
		DataTable rootTable = ((DataSet)rpt.DataSource).Tables["RootReportData"] as DataTable;	
		featureMapImage.Image = rootTable.Rows[0]["FeatureMapImage"] as Bitmap;

	try {
		setFirst("GistPropertyInfoDetails", "districtlot", "districtlot");
		setFirst("GistPropertyInfoDetails", "landdistrict", "landdistrict");

		setFirst("GistPropertySchooldistrictDetails", "schooldistrict", "schooldistrict");

		setVisible("TNRDWaterSystemDetails", "WaterAvailableLabel");
		setVisible("TNRDSewerSystemDetails","SewerAvailableLabel");
		setVisible("TNRDFireProtectionServiceBndDetails","FireProtectionAvailableLabel");
		
		setDefault("GistPropertyAreaDetails");
		
		setJoin("ZoningDetails", "BylawNo", "ZoningBylawData");
		setJoin("ZoningDetails", "Zoning", "ZoningData");
		setVisible("FringeAreaDetails","FringeAreaAvailableLabel");

		setJoin("LakesClassificationDetails", "Name", "LakesNameData");
		setJoin("LakesClassificationDetails", "LakeClassificationLong", "LakesClassificationData");

		setIntersect("LakesClassificationDetails","LakeShoreIntersectLabel");

		setVisible("SiteSpecificZoningFillDetails","SiteZoningAvailableLabel");
		setVisible("DevelopmentPermitAreaDetails","PermitAreaAvailableLabel");
		setVisible("OCPBoundaryOutlineDetails","CommunityAvailablePlanLabel");
		setVisible("OCPDesignationFillDetails","OCPDesignationFillAvailableLabel");
	
		setIntersect("AgriculturalLandReserveDetails","LandReserveIntersectLabel");
		setIntersect("RiparianAreaDetails","RiparianIntersectLabel");
		setIntersect("_200YearFloodplainDetails","FloodPlainIntersectLabel");
	
		setDefault("GistTempestPlanningApplDetails");
		setDefault("GistTempestBuildingPermitDetails");
		setDefault("GistPropertyAssessmentDetails");
		setDefault("GistPropertyActualuseManualclassDetails");

	} catch(Exception e){
		throw new InvalidOperationException("Exception running report: " + e.Message);
	}

	return eof;
}]]></Script>
